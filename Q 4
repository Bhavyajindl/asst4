#include <iostream>
#include <queue>
using namespace std;

void firstNonRepeating(string s) {
    queue<char> q;
    int freq[26] = {0};

    for (char ch : s) {
        // push character and update frequency
        q.push(ch);
        freq[ch - 'a']++;

        // remove all repeating chars from front
        while (!q.empty() && freq[q.front() - 'a'] > 1) {
            q.pop();
        }

        // print current first non-repeating
        if (q.empty())
            cout << -1 << " ";
        else
            cout << q.front() << " ";
    }
    cout << endl;
}

int main() {
    string s;
    cout << "Enter string (lowercase letters): ";
    cin >> s;

    cout << "Output: ";
    firstNonRepeating(s);

    return 0;
}
